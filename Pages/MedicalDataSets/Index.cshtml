            </th>
            <th>
                @Html.DisplayNameFor(model => model.MedicalData[0].DiabetesPedigreeFunction)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MedicalData[0].Prediction)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MedicalData[0].Result)
            </th>
