@page
@model MedicalExpertSystem.Pages.MedicalDataSets.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MedicalData[0].Pregnancies)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MedicalData[0].Glucose)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MedicalData[0].BloodPressure)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MedicalData[0].SkinThickness)
            </th>
